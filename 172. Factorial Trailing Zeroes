//C++
class Solution {
public:
    int trailingZeroes(int n) {
        int s=0;
        while(n/5>0){
            n=n/5;
            s+=n;
        }
        return s;
    }
};

//Python
class Solution:
    def trailingZeroes(self, n: int) -> int:
        t=0
        while int(n/5)>0:
            n=int(n/5)
            t+=n
        return t
