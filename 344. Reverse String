//C++
class Solution {
public:
    void reverseString(vector<char>& s) {
        if(s.size()>1) reverse(s.begin(),s.end());
    }
};

//Python
class Solution:
    def reverseString(self, s: List[str]) -> None:
        """
        Do not return anything, modify s in-place instead.
        """
        if len(s)>1:
            s.reverse()
