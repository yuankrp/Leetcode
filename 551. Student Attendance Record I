//C++
class Solution {
public:
    bool checkRecord(string s) {
        if(s.find("LLL")!=s.npos) return false;
        if(count(s.begin(),s.end(),'A')>1) return false;
        return true;
    }
};

//Python
class Solution:
    def checkRecord(self, s: str) -> bool:
        if "LLL" in s: return False
        if s.count('A')>1: return False
        return True
